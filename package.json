    "node-server": "nodemon ./server/app.js",
    "build": "npm run build-hg && npm run build-hti && npm run build-prd",
    "build-hg": "webpack --env.NODE_ENV=hg --config webpack.dev.js",
    "build-hti": "webpack --env.NODE_ENV=hti --config webpack.dev.js",
    "build-prd": "webpack --env.NODE_ENV=prd --config webpack.prod.js",
    "start-local-hg": "cross-env NODE_ENV=local-hg concurrently --kill-others \"npm:start-local-dev-server\" \"npm:node-server\"",
    "start-local-hti": "cross-env NODE_ENV=local-hti concurrently --kill-others \"npm:start-local-hti-server\" \"npm:node-server\"",
    "start-local-dev-server": "webpack-dev-server --progress --colors --inline --hot --port 9090 --env.NODE_ENV=local-hg --config webpack.dev.js",
    "start-local-hti-server": "webpack-dev-server --progress --colors --inline --hot --port 9090 --env.NODE_ENV=local-hti --config webpack.dev.js"
